                <a class="mdl-navigation__link" href="/#testimonial">Testimonial</a>
                <a class="mdl-navigation__link" href="/offers">Offers</a>
                <a class="mdl-navigation__link" href="/faq">FAQ's</a>
                <a class="mdl-navigation__link" href="#contact">Contact</a>
              </nav>
          </div>
          <main class="mdl-layout__content">
            <a name="top"></a>

              <%= yield %>

            <footer class="ab-footer mdl-mega-footer">
          <div class="mdl-mega-footer--top-section">
            <div class="mdl-mega-footer--left-section">

                <a class="mdl-mega-footer--social-btn btn btn-social-icon btn-linkedin"
                   href="http://www.linkedin.com/in/david-lock-818b88131" target="_new">
