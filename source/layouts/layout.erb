  <body class="<%= page_classes %>">

    <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header ">
          <header class="mdl-layout__header mdl-layout__header--waterfall ab-header ab-header-logo">
              <div class="mdl-layout__header-row ab-logo-row">
                  <span class="mdl-layout__title">
                    <a href="/">
                      <div class="ab-logo"></div>
                      <span class="mdl-layout__title">Auto Brilliance</span>
                    </a>
                  </span>
              </div>
              <div class="mdl-layout__header-row ab-navigation-row mdl-layout--large-screen-only">
                  <nav class="mdl-navigation mdl-typography--body-1-force-preferred-font">
                      <a class="mdl-navigation__link" href="/#why">Why</a>
                      <a class="mdl-navigation__link" href="/#testimonial">Testimonial</a>
                      <a class="mdl-navigation__link" href="/pricing">Pricing</a>
                      <a class="mdl-navigation__link" href="/offers">Offers</a>
                      <a class="mdl-navigation__link" href="/faq">FAQ's</a>
                      <a class="mdl-navigation__link" href="#contact">Contact</a>
                  </nav>
              </div>
          </header>
          <div class="mdl-layout__drawer mdl-layout--small-screen-only ab-drawer">
            <span class="mdl-layout-title">
          <img class="ab-logo-image" src="/images/logo.png">
        </span>
              <nav class="mdl-navigation mdl-typography--body-1-force-preferred-font">
